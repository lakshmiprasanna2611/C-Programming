#include<stdio.h>
int fibo(int n)
{
	static int n1=0,n2=1,n3;
	if(n>0){
		n3=n1+n2;
		n1=n2;
		n2=n3;
		printf("%d\n",n3);
		fibo(n-1);
	}}
	int main()
	{
		int n;
    	printf("enter no of elements:\n");
	    scanf("%d",&n);
	    printf("fibonacci series:\n");
	printf("%d\n%d\n",0,1);
	fibo(n-2);
	return 0;
}
