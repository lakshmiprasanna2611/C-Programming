#include <stdio.h>
int main()
{
    int i,n, sum = 0,arr[10];  
    printf("enter the size:\n");
    scanf("%d",&n);
    printf("enter the elements:\n");
    for(i=0;i<n;i++)
    scanf("%d",&arr[i]);
    for (i = 0; i < n; i++)
    {
        sum += arr[i]; 
    }
    printf("Sum of all elements stored in the array is : %d\n\n", sum);
	return 0;
}
